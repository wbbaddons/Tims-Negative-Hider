			<environment>user</environment>
			<templatename>headInclude</templatename>
			<eventname>javascriptInclude</eventname>
			<templatecode><![CDATA[<script data-relocate="true" src="{$__wcf->getPath()}js/be.bastelstu.wcf.hideDisliked{if !ENABLE_DEBUG_MODE}.min{/if}.js"></script>]]></templatecode>
		</templatelistener>
		
